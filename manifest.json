{
	"manifest_version": 2,
  "name": "VOD TVP Film bez reklam",
  "description": "Te rozszerzenie pozwala oglądać filmy z serwisu VOD TVP z pominięciem reklam",
  "version": "1.2",
	"background": {
		"scripts": ["event.js"],
		"persistent": false
	},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
		"tabs", 
    "<all_urls>"
  ]
}
